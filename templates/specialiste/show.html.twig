{% extends 'base.html.twig' %}

{% block title %}Specialiste{% endblock %}

{% block body %}
    <h1>Specialiste</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ specialiste.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ specialiste.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ specialiste.roles ? specialiste.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ specialiste.password }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ specialiste.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ specialiste.prenom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_specialiste_index') }}">back to list</a>

    <a href="{{ path('app_specialiste_edit', {'id': specialiste.id}) }}">edit</a>

    {{ include('specialiste/_delete_form.html.twig') }}
{% endblock %}
